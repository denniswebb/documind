{
  "name": "@dennis-webb/documind",
  "version": "1.1.21",
  "type": "module",
  "description": "IDE-native documentation system with AI-powered slash commands",
  "main": "install.js",
  "bin": {
    "documind": "./install.js"
  },
  "scripts": {
    "test": "node --test tests/unit/*.test.js",
    "test:integration": "node --test tests/integration/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/**/*.test.js",
    "test:coverage:lcov": "node --test --experimental-test-coverage --test-reporter=lcov tests/**/*.test.js > coverage/lcov.info",
    "test:coverage:html": "node --test --experimental-test-coverage --test-reporter=spec tests/**/*.test.js && mkdir -p coverage",
    "test:all": "npm test && npm run test:integration",
    "test:watch": "node --test --watch tests/unit/*.test.js",
    "test:matrix": "npm run test:node16 && npm run test:node18 && npm run test:node20 && npm run test:node21",
    "test:node16": "NODE_VERSION=16 npm run test:all",
    "test:node18": "NODE_VERSION=18 npm run test:all",
    "test:node20": "NODE_VERSION=20 npm run test:all",
    "test:node21": "NODE_VERSION=21 npm run test:all",
    "test:performance": "node --test tests/performance/*.test.js",
    "coverage:check": "node --test --experimental-test-coverage tests/**/*.test.js | grep -E 'Coverage|%'",
    "coverage:validate": "node tests/coverage-validation.js",
    "coverage:report": "npm run test:coverage && npm run coverage:validate",
    "documind:init": "node install.js init",
    "version": "node -e \"console.log(require('./package.json').version)\""
  },
  "keywords": [
    "documentation",
    "ai",
    "slash-commands",
    "ide",
    "cursor",
    "copilot",
    "claude",
    "gemini",
    "markdown",
    "docs"
  ],
  "author": "Dennis Webb <dennis@denniswebb.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/denniswebb/documind.git"
  },
  "homepage": "https://github.com/denniswebb/documind#readme",
  "bugs": {
    "url": "https://github.com/denniswebb/documind/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src/",
    "install.js",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "yaml": "^2.8.1",
    "@scil/mdsplit-js": "^100.4.1"
  },
  "optionalDependencies": {
    "tiktoken": "^1.0.15"
  }
}
